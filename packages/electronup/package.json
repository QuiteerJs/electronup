{
  "name": "electronup",
  "version": "0.1.1",
  "description": "融合构建 electron 应用需要的构建工具,保留原有配置习惯的命令行工具 ",
  "author": "quiteer",
  "license": "MIT",
  "homepage": "https://github.com/QuiteerJs/electronup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuiteerJs/electronup.git"
  },
  "bugs": {
    "url": "https://github.com/QuiteerJs/electronup/issues"
  },
  "keywords": [
    "vite",
    "tsup",
    "electron-builder",
    "electron"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/client/index.js",
  "module": "dist/client/index.mjs",
  "types": "dist/client/index.d.ts",
  "bin": {
    "electronup": "dist/bin/electronup.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "update-version": "bumpp package.json"
  },
  "peerDependencies": {
    "@types/node": ">= 18",
    "node": ">= 18",
    "vue": ">= 3"
  },
  "dependencies": {
    "@quiteer/parser-config": "^1.0.3",
    "@types/prompts": "^2.4.4",
    "cac": "^6.7.14",
    "dotenv": "^16.3.1",
    "electron": "^22.3.6",
    "electron-builder": "^24.9.1",
    "fs-extra": "^11.2.0",
    "kolorist": "^1.8.0",
    "portfinder": "^1.0.32",
    "prompts": "^2.4.2",
    "tsup": "^7.3.0",
    "vite": "^4.5.1",
    "yaml": "^2.3.4"
  }
}
