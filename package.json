{
  "name": "electronup-monorepo",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "description": "融合构建 electron 应用需要的构建工具,保留原有配置习惯的命令行工具 ",
  "author": "quiteer",
  "license": "MIT",
  "homepage": "https://github.com/QuiteerJs/electronup#readme",
  "scripts": {
    "work:dev": "pnpm --filter './packages/*' dev",
    "work:build": "pnpm --filter './packages/*' build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "template:run": "pnpm --filter './template/*' dev",
    "lint": "eslint . --fix"
  },
  "devDependencies": {
    "@quiteer/eslint-config": "^0.0.3",
    "@quiteer/ts-config": "^0.0.6",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^20.3.2",
    "bumpp": "^9.1.1",
    "eslint": "^8.46.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitepress": "1.0.0-beta.7",
    "vitest": "^0.34.1"
  }
}
